{
  "name": "react-data-grid-plus",
  "version": "1.0.0-beta.48",
  "license": "MIT",
  "description": "My customized react-data-grid with original internal names",
  "keywords": [
    "react",
    "data grid"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/TalentSmallPanda/react-data-grid-plus"
  },
  "bugs": "https://github.com/TalentSmallPanda/react-data-grid-plus/issues",
  "type": "module",
  "exports": {
    "./lib/styles.css": "./lib/styles.css",
    ".": {
      "types": "./lib/index.d.ts",
      "module": "./lib/bundle.js",
      "require": "./lib/bundle.cjs",
      "default": "./lib/bundle.js"
    }
  },
  "browser": "./lib/bundle.js",
  "main": "./lib/bundle.cjs",
  "module": "./lib/bundle.js",
  "types": "./lib/index.d.ts",
  "files": [
    "lib"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "start": "vite serve --clearScreen false",
    "preview": "vite preview",
    "build:website": "vite build",
    "build": "rollup --config --no-stdin",
    "build:types": "tsc -p tsconfig.lib.json && api-extractor run --local --verbose",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "format": "biome format --write",
    "check": "biome check --error-on-warnings",
    "biome:ci": "biome ci --error-on-warnings",
    "eslint": "eslint --max-warnings 0 --cache --cache-location .cache/eslint --cache-strategy content",
    "eslint:fix": "node --run eslint -- --fix",
    "prettier:check": "prettier --check .",
    "prettier:format": "prettier --write .",
    "typecheck": "tsc --build",
    "prepublishOnly": "npm install && node --run build && node --run build:types",
    "postpublish": "git push --follow-tags origin HEAD"
  },
  "dependencies": {
    "clsx": "^2.0.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.25.9",
    "@babel/preset-typescript": "^7.26.0",
    "@babel/runtime": "^7.26.7",
    "@biomejs/biome": "1.9.4",
    "@csstools/postcss-cascade-layers": "^5.0.1",
    "@eslint/compat": "^1.2.5",
    "@eslint/markdown": "^6.2.2",
    "@faker-js/faker": "^9.4.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.1",
    "@linaria/core": "^6.0.0",
    "@microsoft/api-extractor": "^7.49.2",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@tanstack/react-router": "^1.98.3",
    "@tanstack/router-plugin": "^1.98.3",
    "@testing-library/dom": "^10.1.0",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^22.12.0",
    "@types/react": "^18.3.9",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/browser": "3.0.3",
    "@vitest/coverage-v8": "3.0.3",
    "@vitest/eslint-plugin": "^1.1.25",
    "@wyw-in-js/rollup": "^0.5.0",
    "@wyw-in-js/vite": "^0.5.0",
    "autoprefixer": "^10.4.20",
    "babel-plugin-optimize-clsx": "^2.6.2",
    "browserslist": "^4.24.4",
    "cssnano": "^7.0.6",
    "eslint": "^9.19.0",
    "eslint-plugin-jest-dom": "^5.5.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-compiler": "^19.0.0-beta-27714ef-20250124",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-hooks-extra": "^1.25.0",
    "eslint-plugin-sonarjs": "^3.0.1",
    "eslint-plugin-testing-library": "^7.1.1",
    "jspdf": "^2.5.1",
    "jspdf-autotable": "^3.5.23",
    "playwright": "^1.50.0",
    "postcss": "^8.5.1",
    "postcss-import": "^16.1.0",
    "postcss-nested": "^7.0.2",
    "prettier": "3.4.2",
    "react": "^18.3.1",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.3.1",
    "rolldown": "^1.0.0-beta.3",
    "rollup": "^4.0.2",
    "rollup-plugin-postcss": "^4.0.2",
    "typescript": "~5.7.3",
    "vite": "^6.0.11",
    "vitest": "3.0.3",
    "vitest-browser-react": "^0.0.4"
  },
  "peerDependencies": {
    "react": "^18.0 || ^19.0",
    "react-dom": "^18.0 || ^19.0"
  }
}
